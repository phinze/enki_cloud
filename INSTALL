
apt-get install ruby

gzip -dc rubygems-1.3.1.tgz |tar xf -
cd rubygems-1.3.1/
ruby setup.rb --prefix=/tmp/rg-chad --destdir=/tmp/rg-chad/destdir
cd /tmp/rg-chad/
sudo ls -al  /usr/lib/ruby/1.8/
sudo ls -al  /usr/lib/ruby/
sudo ls -al  /usr/lib/ruby/gems
sudo mkdir  /usr/lib/ruby/gems/1.8/
sudo mkdir -p  /usr/lib/ruby/gems/1.8/
cd rg-chad/bin
cd ../lib
grep -i rg-chad *
sudo cp -rpd * /usr/lib/ruby/1.8/
sudo cp -rpd ../bin/gem1.8 /usr/bin/
sudo ln -s /usr/bin/gem1.8 /usr/bin/gem

gem list
sudo apt-get install rdoc1.8 
sudo gem install rake-0.8.3.gem 
sudo gem install ruby-postgres-0.7.1.2006.04.06.gem 
sudo apt-get install libpgsql-ruby1.8
sudo apt-get install libdbd-pg-ruby
sudo apt-get install irb

